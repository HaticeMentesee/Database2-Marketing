<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #2f343f;" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="marketingproject.controllers.LoginPageController">
   <children>
      <Label fx:id="SignIn_Label" layoutX="351.0" layoutY="48.0" text="Sign In" textFill="#e2dddd">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
      <TextField fx:id="UserName_TextField" layoutX="250.0" layoutY="136.0" prefHeight="32.0" prefWidth="300.0" promptText="User Name" />
      <PasswordField fx:id="Password_TextField" layoutX="250.0" layoutY="199.0" prefHeight="32.0" prefWidth="300.0" promptText="Password" />
      <Button fx:id="Login_Button" layoutX="365.0" layoutY="293.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="71.0" text="LOGIN">
         <font>
            <Font size="14.0" />
         </font></Button>
      <Label fx:id="SignUp_Label" layoutX="371.0" layoutY="334.0" onMouseClicked="#GoToSignUp" style="-fx-underline: true;" text="Sign Up" textFill="#efe9e9">
         <font>
            <Font size="17.0" />
         </font>
         <effect>
            <Blend />
         </effect>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
      <CheckBox fx:id="cb_employee_login" layoutX="250.0" layoutY="255.0" mnemonicParsing="false" text="Employee Login" textFill="WHITE" />
      <Label fx:id="lbl_error" layoutX="250.0" layoutY="105.0" prefHeight="18.0" prefWidth="300.0" textFill="RED">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
