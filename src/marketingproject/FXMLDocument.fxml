<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="680.0" style="-fx-background-color: #232323;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="marketingproject.FXMLDocumentController">
   <children>
      <Label fx:id="SignIn_Label" layoutX="297.0" layoutY="50.0" text="Sign In" textFill="#e2dddd">
         <font>
            <Font size="32.0" />
         </font>
      </Label>
      <TextField fx:id="UserName_TextField" layoutX="196.0" layoutY="121.0" prefHeight="32.0" prefWidth="300.0" promptText="User Name" />
      <PasswordField fx:id="Password_TextField" layoutX="196.0" layoutY="184.0" prefHeight="32.0" prefWidth="300.0" promptText="Password" />
      <Button fx:id="Login_Button" layoutX="311.0" layoutY="250.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="71.0" text="LOGIN" />
      <Label fx:id="SignUp_Label" layoutX="310.0" layoutY="311.0" onMouseClicked="#GoToSignUp" text="Sign Up" textFill="#efe9e9">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
